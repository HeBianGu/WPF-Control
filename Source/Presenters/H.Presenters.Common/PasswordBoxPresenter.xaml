<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:h="https://github.com/HeBianGu"
                    xmlns:local="clr-namespace:H.Presenters.Common">
    <DataTemplate DataType="{x:Type local:PasswordBoxPresenter}">
        <PasswordBox Height="Auto"
                     MinHeight="{DynamicResource {x:Static h:LayoutKeys.ItemHeight}}"
                     Margin="5"
                     Cattach.Password="{Binding Password, Mode=TwoWay}"
                     ToolTip="{Binding Password}" />
    </DataTemplate>
</ResourceDictionary>