<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:b="http://schemas.microsoft.com/xaml/behaviors"
                    xmlns:h="https://github.com/HeBianGu"
                    xmlns:lc="clr-namespace:H.Modules.Login.Commands"
                    xmlns:local="clr-namespace:H.Modules.Login">
    <DataTemplate DataType="{x:Type local:CurrentUserViewPresenter}">
        <DockPanel>
            <DockPanel HorizontalAlignment="Right"
                       DockPanel.Dock="Top">
                <FontIconButton Command="{lc:LogoutCommand}"
                                Style="{DynamicResource {x:Static FontIconButtonKeys.Command}}" />
                <FontIconButton Command="{lc:SwitchUserCommand}"
                                Style="{DynamicResource {x:Static FontIconButtonKeys.Command}}" />
            </DockPanel>
            <Form SelectObject="{Binding Source={Ioc Type={x:Type ILoginService}}, Path=User}"
                  UsePropertyView="True" />
        </DockPanel>
    </DataTemplate>
</ResourceDictionary>