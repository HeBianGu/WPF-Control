<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:local="clr-namespace:H.Modules.Login"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DataTemplate DataType="{x:Type local:LoginViewPresenter}">
        <!--<TextBlock Text="{Ioc Type={x:Type local:LoginOptions}}" FontSize="80"/>-->
        <UniformGrid>
            <TextBlock Text="{Binding Name}" FontSize="20"/>
            <!--<TextBlock Text="{Binding Source={x:Static local:LoginOptions.Instance},Path=MyName}"/>-->
            <TextBlock Text="{Binding Source={x:Static local:LoginOptions.Instance},Path=Name}"/>

        </UniformGrid>
    </DataTemplate>
</ResourceDictionary>