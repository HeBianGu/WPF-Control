    <Color x:Key="{ComponentResourceKey ResourceId=S.Color.Dark.28, TypeInTargetAssembly={x:Type ColorKeys}}">#FF000000</Color>
    <!--  Fluent UI Semantic Colors  -->
    <Color x:Key="{ComponentResourceKey ResourceId=S.Color.Primary, TypeInTargetAssembly={x:Type ColorKeys}}">#0078D4</Color>
    <Color x:Key="{ComponentResourceKey ResourceId=S.Color.Accent, TypeInTargetAssembly={x:Type ColorKeys}}">#0078D4</Color>
    <Color x:Key="{ComponentResourceKey ResourceId=S.Color.Error, TypeInTargetAssembly={x:Type ColorKeys}}">#A4262C</Color>
    <Color x:Key="{ComponentResourceKey ResourceId=S.Color.Success, TypeInTargetAssembly={x:Type ColorKeys}}">#107C10</Color>
</ResourceDictionary>