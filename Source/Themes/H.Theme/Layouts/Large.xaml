<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ll="clr-namespace:H.Themes.Layouts"
                    xmlns:local="clr-namespace:H.Themes"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:Double x:Key="{ComponentResourceKey ResourceId=S.Layout.WindowCaptionHeight, TypeInTargetAssembly={x:Type ll:LayoutKeys}}">50</system:Double>
    <system:Double x:Key="{ComponentResourceKey ResourceId=S.Layout.ItemHeight, TypeInTargetAssembly={x:Type ll:LayoutKeys}}">40</system:Double>
    <system:Double x:Key="{ComponentResourceKey ResourceId=S.Layout.IconHeight, TypeInTargetAssembly={x:Type ll:LayoutKeys}}">15</system:Double>
    <system:Double x:Key="{ComponentResourceKey ResourceId=S.Layout.RowHeight, TypeInTargetAssembly={x:Type ll:LayoutKeys}}">45</system:Double>
    <CornerRadius x:Key="{ComponentResourceKey ResourceId=S.Layout.CornerRadius, TypeInTargetAssembly={x:Type ll:LayoutKeys}}">4</CornerRadius>
    <Thickness x:Key="{ComponentResourceKey ResourceId=S.Layout.Padding, TypeInTargetAssembly={x:Type ll:LayoutKeys}}">10 0</Thickness>
    <Thickness x:Key="{ComponentResourceKey ResourceId=S.Layout.Margin, TypeInTargetAssembly={x:Type ll:LayoutKeys}}">10 6</Thickness>
</ResourceDictionary>