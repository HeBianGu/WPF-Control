<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:lc="clr-namespace:H.Themes.Colors"
                    xmlns:local="clr-namespace:H.Themes"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=S.Brush.TextBackground,
                                                  TypeInTargetAssembly={x:Type local:BrushKeys}}"
                     po:Freeze="True"
                     Color="{DynamicResource {x:Static lc:ColorKeys.Background}}" />
</ResourceDictionary>