                            <Button Width="100"
                                    Height="100"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Content="aa" />
                        </Grid>
                    </h:Zoombox>
                </DockPanel>
            </TabItem>
            <TabItem Header="日志测试">
                <UniformGrid>
                    <Button Command="{LogCommand Message=运行日志, Type=Info}" Content="Info" />
                    <Button Command="{LogCommand Message=错误日志, Type=Error}" Content="Error" />
                    <Button Command="{LogCommand Message=调试日志, Type=Debug}" Content="Debug" />
                    <Button Command="{LogCommand Message=警告日志, Type=Warn}" Content="Warn" />
                    <Button Command="{LogCommand Message=严重日志, Type=Fatal}" Content="Fatal" />
                </UniformGrid>
            </TabItem>
            <TabItem Header="ITree">
                <TabControl>
                    <TabItem Header="类继承关系">
                        <TreeView ItemsSource="{ClassTypeTreeDataProvider Type={x:Type FrameworkElement}}">
                            <TreeView.ItemTemplate>
                                <HierarchicalDataTemplate ItemsSource="{Binding Nodes}">
                                    <TextBlock x:Name="a"
                                               VerticalAlignment="Center"
                                               Text="{Binding Model.Name}" />
                                </HierarchicalDataTemplate>
                            </TreeView.ItemTemplate>
                            <TreeView.ItemContainerStyle>
                                <Style BasedOn="{StaticResource {x:Static h:TreeViewItemKeys.Default}}" TargetType="TreeViewItem">
                                    <Setter Property="h:BehvaiorAttach.Behaviors">
                                        <Setter.Value>
                                            <h:Behaviors>
                                                <h:TreeViewItemLazyLoadBehavior Tree="{ClassTypeTree}" />
                                            </h:Behaviors>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </TreeView.ItemContainerStyle>
                        </TreeView>
                    </TabItem>
                    <TabItem Header="资源管理器">
                        <TreeView ItemsSource="{ExploreTreeDataProvider}">
                            <TreeView.ItemTemplate>
                                <HierarchicalDataTemplate ItemsSource="{Binding Nodes}">
                                    <TextBlock x:Name="a"
                                               VerticalAlignment="Center"
                                               Text="{Binding Model.Name}" />
                                </HierarchicalDataTemplate>
                            </TreeView.ItemTemplate>
                            <TreeView.ItemContainerStyle>
                                <Style BasedOn="{StaticResource {x:Static h:TreeViewItemKeys.Default}}" TargetType="TreeViewItem">
                                    <Setter Property="h:BehvaiorAttach.Behaviors">
                                        <Setter.Value>
                                            <h:Behaviors>
                                                <h:TreeViewItemLazyLoadBehavior>
                                                    <h:TreeViewItemLazyLoadBehavior.Tree>
                                                        <h:ExploreTree />
                                                    </h:TreeViewItemLazyLoadBehavior.Tree>
                                                </h:TreeViewItemLazyLoadBehavior>
                                            </h:Behaviors>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </TreeView.ItemContainerStyle>
                        </TreeView>
                    </TabItem>
                </TabControl>
            </TabItem>
            <TabItem Header="TreeListView">
                <h:TreeListView ItemsSource="{ClassTypeTreeDataProvider Type={x:Type FrameworkElement}, IsRecursion=True}">
                    <h:TreeListView.Columns>
                        <GridViewColumnCollection>
                            <GridViewColumn Width="300" Header="Name">
                                <GridViewColumn.CellTemplate>
                                    <DataTemplate>
                                        <DockPanel>
                                            <ToggleButton Margin="{Binding Path=Level, Converter={GetLevelToIndentConverter}, RelativeSource={RelativeSource AncestorType={x:Type h:TreeListViewItem}}}" Style="{DynamicResource {x:Static h:ToggleButtonKeys.Tree}}" />
                                            <TextBlock Margin="5,0">
                                                <Run Text="{Binding Model.Name, Mode=OneWay}" />
                                            </TextBlock>
                                        </DockPanel>
                                    </DataTemplate>
                                </GridViewColumn.CellTemplate>
                            </GridViewColumn>
                            <GridViewColumn DisplayMemberBinding="{Binding Model.IsClass, Mode=OneWay}" Header="IsClass" />
                            <GridViewColumn DisplayMemberBinding="{Binding Model.IsInterface, Mode=OneWay}" Header="IsInterface" />
                            <GridViewColumn DisplayMemberBinding="{Binding Model.Assembly, Mode=OneWay}" Header="Assembly" />
                        </GridViewColumnCollection>
                    </h:TreeListView.Columns>
                    <TreeView.ItemTemplate>
                        <HierarchicalDataTemplate ItemsSource="{Binding Nodes}">
                            <TextBlock VerticalAlignment="Center" Text="{Binding Model.Name}" />
                        </HierarchicalDataTemplate>
                    </TreeView.ItemTemplate>
                </h:TreeListView>
            </TabItem>
            <TabItem Header="Step">
                <TabControl>
                    <TabItem Header="Horizontal">
                        <h:Step Height="Auto" Cattach.ItemHeight="Auto">
                            <h:StepItemPresenter DisplayName="1"
                                                 Message="成功"
                                                 State="Success" />
                            <h:StepItemPresenter DisplayName="2"
                                                 Message="成功"
                                                 State="Success" />
                            <h:StepItemPresenter DisplayName="3"
                                                 Message="成功"
                                                 State="Success" />
                            <h:StepItemPresenter DisplayName="4"
                                                 Message="失败"
                                                 State="Error" />
                            <h:StepItemPresenter DisplayName="5"
                                                 Message="正在运行"
                                                 State="Running" />
                            <h:StepItemPresenter DisplayName="6"
                                                 Message=""
                                                 State="Ready" />
                            <h:StepItemPresenter DisplayName="7"
                                                 Message=""
                                                 State="Ready" />
                        </h:Step>
                    </TabItem>
                    <TabItem Header="Vertical">
                        <h:Step Height="Auto">
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <StackPanel />
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                            <h:StepItemVerticalPresenter DisplayName="1"
                                                         Message="成功"
                                                         State="Success" />
                            <h:StepItemVerticalPresenter DisplayName="2"
                                                         Message="成功"
                                                         State="Success" />
                            <h:StepItemVerticalPresenter DisplayName="3"
                                                         Message="成功"
                                                         State="Success" />
                            <h:StepItemVerticalPresenter DisplayName="4"
                                                         Message="失败"
                                                         State="Error" />
                            <h:StepItemVerticalPresenter DisplayName="5"
                                                         Message="正在运行"
                                                         State="Running" />
                            <h:StepItemVerticalPresenter DisplayName="6"
                                                         Message=""
                                                         State="Ready" />
                            <h:StepItemVerticalPresenter DisplayName="7"
                                                         Message=""
                                                         State="Ready" />
                        </h:Step>
                    </TabItem>
                </TabControl>
            </TabItem>
            <TabItem Header="ColorBox">
                <TabControl>
                    <TabItem Header="ColorBox">
                        <h:ColorBox ItemsSource="{GetDepthColors}" />
                    </TabItem>
                    <TabItem Header="Available">
                        <h:ColorBox Height="Auto" ItemsSource="{GetAvailableColors}">
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <UniformGrid />
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                        </h:ColorBox>
                    </TabItem>
                    <TabItem Header="Standard">
                        <h:ColorBox ItemsSource="{GetStandardColors}" />
                    </TabItem>
                    <TabItem Header="System">
                        <h:ColorBox Height="Auto" ItemsSource="{GetSystemColors}">
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <UniformGrid />
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                        </h:ColorBox>
                    </TabItem>
                </TabControl>
            </TabItem>
            <TabItem Header="PrintBox">
                <h:PrintBox>
                    <h:PrintPage>
                        <ListBox Background="White"
                                 Cattach.ItemHeight="Auto"
                                 ItemsSource="{GetStudents Count=100}" />
                    </h:PrintPage>
                </h:PrintBox>
            </TabItem>
            <TabItem Header="TagBox">
                <DockPanel>
                    <StackPanel Width="250" DockPanel.Dock="Right">
                        <GroupBox Header="默认标签">
                            <h:TagBox Tags="{Binding SelectedTestViewModel.Value, Mode=TwoWay}" />
                        </GroupBox>

                        <GroupBox Header="分组标签">
                            <h:TagBox GroupName="Test" Tags="{Binding SelectedTestViewModel.Value1, Mode=TwoWay}" />
                        </GroupBox>
                    </StackPanel>
                    <StackPanel Width="250" DockPanel.Dock="Left">
                        <GroupBox Header="默认标签">
                            <h:TagFilterBox x:Name="tagfb" PropertyName="Value" />
                        </GroupBox>
                        <GroupBox Header="分组标签">
                            <h:TagFilterBox x:Name="tagfb1"
                                            GroupName="Test"
                                            PropertyName="Value1" />
                        </GroupBox>
                    </StackPanel>
                    <DataGrid SelectedItem="{Binding SelectedTestViewModel}">
                        <Behaviors:Interaction.Behaviors>
                            <h:DataGridAutoColumnBehavior DataGridLength="*"
                                                          Type="{x:Type h:TestViewModel}"
                                                          UsePropertyNames="Value Value1" />
                            <h:ItemsControlFilterBehavior Filter="{Binding ElementName=tagfb, Path=Filter}"
                                                          Filter1="{Binding ElementName=tagfb1, Path=Filter}"
                                                          ItemsSource="{Binding TestViewModels}" />
                        </Behaviors:Interaction.Behaviors>
                    </DataGrid>
                </DockPanel>
            </TabItem>
        </TabControl>
    </Grid>
</h:MainWindow>
