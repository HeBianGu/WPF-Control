    <Grid>
        <TabControl>
            <TabItem Header="GridSplitterBox">
                <Grid>
                    <TabControl>
                        <TabItem Header="Left">
                            <h:GridSplitterBox ID="Left">
                                <Border>
                                    <Grid Background="Red">
                                        <TextBlock FontSize="100" Text="Center" />
                                    </Grid>
                                </Border>
                                <h:GridSplitterBox.MenuContent>
                                    <Border Background="Yellow">
                                        <TextBlock FontSize="100" Text="Menu" />
                                    </Border>
                                </h:GridSplitterBox.MenuContent>
                            </h:GridSplitterBox>
                        </TabItem>

                        <TabItem Header="right">
                            <h:GridSplitterBox ID="right" Style="{DynamicResource {x:Static h:GridSplitterBox.RightKey}}">
                                <Border>
                                    <Grid>
                                        <TextBlock FontSize="100" Text="Center" />
                                    </Grid>
                                </Border>
                                <h:GridSplitterBox.MenuContent>
                                    <Border>
                                        <TextBlock FontSize="100" Text="Menu" />
                                    </Border>
                                </h:GridSplitterBox.MenuContent>
                            </h:GridSplitterBox>
                        </TabItem>

                        <TabItem Header="top">
                            <h:GridSplitterBox ID="top" Style="{DynamicResource {x:Static h:GridSplitterBox.TopKey}}">
                                <Border>
                                    <Grid>
                                        <TextBlock FontSize="100" Text="Center" />
                                    </Grid>
                                </Border>
                                <h:GridSplitterBox.MenuContent>
                                    <Border>
                                        <TextBlock FontSize="100" Text="Menu" />
                                    </Border>
                                </h:GridSplitterBox.MenuContent>
                            </h:GridSplitterBox>
                        </TabItem>

                        <TabItem Header="bottom">
                            <h:GridSplitterBox ID="bottom" Style="{DynamicResource {x:Static h:GridSplitterBox.BottomKey}}">
                                <Border>
                                    <Grid>
                                        <TextBlock FontSize="100" Text="Center" />
                                    </Grid>
                                </Border>
                                <h:GridSplitterBox.MenuContent>
                                    <Border>
                                        <TextBlock FontSize="100" Text="Menu" />
                                    </Border>
                                </h:GridSplitterBox.MenuContent>
                            </h:GridSplitterBox>
                        </TabItem>
                    </TabControl>
                </Grid>
            </TabItem>
            <TabItem Header="HorseTextBlock">
                <DockPanel>
                    <TextBox DockPanel.Dock="Bottom" Text="{Binding ElementName=hourse, Path=Text, UpdateSourceTrigger=PropertyChanged}" />
                    <h:HorseTextBlock x:Name="hourse"
                                      ClipToBounds="True"
                                      FontSize="55"
                                      FontWeight="Black"
                                      Foreground="Red"
                                      RepeatBehavior="5x"
                                      Text="我是用来测试跑马灯的文本"
                                      From="900"
                                      To="-500"
                                      Duration="00:00:03" />
                </DockPanel>
            </TabItem>
            <TabItem Header="FilterBox">
                <DockPanel>
                    <DockPanel DockPanel.Dock="Top" LastChildFill="False">
                        <h:PropertyFilterBox x:Name="fb"
                                             Margin="5"
                                             Type="{x:Type h:Student}" />
                        <h:TextFilterBox x:Name="tfb"
                                         Width="200"
                                         DockPanel.Dock="Right" />
                    </DockPanel>
                    <DockPanel DockPanel.Dock="Top">
                        <BulletDecorator Margin="5,0">
                            <BulletDecorator.Bullet>
                                <TextBlock VerticalAlignment="Center" Text="用户姓名  " />
                            </BulletDecorator.Bullet>
                            <h:SelectionFilterBox x:Name="sfb"
                                                  MaxHeight="100"
                                                  Margin="0,5"
                                                  HorizontalAlignment="Stretch"
                                                  HorizontalContentAlignment="Stretch"
                                                  Datas="{Binding Students}"
                                                  DisplayName="选择筛选器筛选出来的用户数量"
                                                  PropertyName="Name"
                                                  SelectionMode="Multiple"
                                                  Type="{x:Type h:Student}" />
                        </BulletDecorator>
                    </DockPanel>
                    <DockPanel DockPanel.Dock="Top">
                        <BulletDecorator Margin="5,0">
                            <BulletDecorator.Bullet>
                                <TextBlock VerticalAlignment="Center" Text="用户班级  " />
                            </BulletDecorator.Bullet>
                            <h:SelectionFilterBox x:Name="sfb1"
                                                  MaxHeight="100"
                                                  Margin="0,5"
                                                  HorizontalAlignment="Stretch"
                                                  HorizontalContentAlignment="Stretch"
                                                  Datas="{Binding Students}"
                                                  PropertyName="Class"
                                                  SelectionMode="Multiple"
                                                  Type="{x:Type h:Student}" />
                        </BulletDecorator>
                    </DockPanel>
                    <DockPanel DockPanel.Dock="Top">
                        <BulletDecorator Margin="5,0">
                            <BulletDecorator.Bullet>
                                <TextBlock VerticalAlignment="Center" Text="是否启用  " />
                            </BulletDecorator.Bullet>

                            <h:SelectionFilterBox x:Name="sfb2"
                                                  MaxHeight="100"
                                                  Margin="0,5"
                                                  HorizontalAlignment="Stretch"
                                                  HorizontalContentAlignment="Stretch"
                                                  Datas="{Binding Students}"
                                                  PropertyName="IsEnbled"
                                                  SelectionMode="Multiple"
                                                  Type="{x:Type h:Student}">
                                <ItemsControl.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding ., Converter={h:GetBoolToValueConverter TrueValue=启用, FalseValue=禁用, DefaultValue=全选}}" />
                                    </DataTemplate>
                                </ItemsControl.ItemTemplate>
                            </h:SelectionFilterBox>
                        </BulletDecorator>
                    </DockPanel>
                    <TextBlock Margin="10" DockPanel.Dock="Bottom">
                        <Run Text=" 合计：" />
                        <Run Text="{Binding ElementName=dg, Path=ItemsSource.Count, Mode=OneWay}" />
                    </TextBlock>

                    <TextBlock Margin="10" DockPanel.Dock="Bottom">
                        <Behaviors:Interaction.Behaviors>
                            <h:TextBlockCountBehavior ItemsSource="{Binding ElementName=dg, Path=ItemsSource}"
                                                      PropertyName="Name"
                                                      Type="{x:Type h:Student}"
                                                      Value="Tom" />
                        </Behaviors:Interaction.Behaviors>
                    </TextBlock>

                    <TextBlock Margin="10" DockPanel.Dock="Bottom">
                        <Behaviors:Interaction.Behaviors>
                            <h:TextBlockFilterCountBehavior DisplayFilter="{Binding ElementName=sfb, Path=Filter}" ItemsSource="{Binding ElementName=dg, Path=ItemsSource}" />
                        </Behaviors:Interaction.Behaviors>
                    </TextBlock>

                    <TextBlock Margin="10" DockPanel.Dock="Bottom">
                        <Behaviors:Interaction.Behaviors>
                            <h:TextBlockFilterCountBehavior ItemsSource="{Binding ElementName=dg, Path=ItemsSource}">
                                <h:TextBlockFilterCountBehavior.DisplayFilter>
                                    <h:StringFilter Name="Name"
                                                    DisplayName="姓名=Tom"
                                                    Operate="{x:Static h:FilterOperate.Equals}"
                                                    Value="Tom" />
                                </h:TextBlockFilterCountBehavior.DisplayFilter>
                            </h:TextBlockFilterCountBehavior>
                        </Behaviors:Interaction.Behaviors>
                    </TextBlock>
                    <TextBlock Margin="10" DockPanel.Dock="Bottom">
                        <Behaviors:Interaction.Behaviors>
                            <h:TextBlockFiltersCountBehavior ItemsSource="{Binding ElementName=dg, Path=ItemsSource}">
                                <h:TextBlockFiltersCountBehavior.DisplayFilters>
                                    <h:IntFilter Name="Age"
                                                 DisplayName="年龄大于20岁"
                                                 Operate="{x:Static h:FilterOperate.Greater}"
                                                 Value="20" />
                                    <h:StringFilter Name="Class"
                                                    DisplayName="班级=3班"
                                                    Operate="{x:Static h:FilterOperate.Equals}"
                                                    Value="3班" />
                                </h:TextBlockFiltersCountBehavior.DisplayFilters>
                            </h:TextBlockFiltersCountBehavior>
                        </Behaviors:Interaction.Behaviors>
                    </TextBlock>
                    <DataGrid x:Name="dg">
                        <Behaviors:Interaction.Behaviors>
                            <h:DataGridAutoColumnBehavior DataGridLength="*" Type="{x:Type h:Student}" />
                            <h:ItemsControlFilterBehavior Filter="{Binding ElementName=fb, Path=Filter}"
                                                          Filter1="{Binding ElementName=tfb, Path=Filter}"
                                                          Filter2="{Binding ElementName=sfb, Path=Filter}"
                                                          Filter3="{Binding ElementName=sfb1, Path=Filter}"
                                                          Filter4="{Binding ElementName=sfb2, Path=Filter}"
                                                          ItemsSource="{Binding Students}" />
                        </Behaviors:Interaction.Behaviors>
                    </DataGrid>
                </DockPanel>
            </TabItem>
            <TabItem Header="OrderBox">
                <DockPanel>
                    <DockPanel DockPanel.Dock="Top" LastChildFill="False">
                        <h:PropertyOrderBox x:Name="ob"
                                            Margin="5"
                                            Type="{x:Type h:Student}" />
                    </DockPanel>
                    <DataGrid>
                        <Behaviors:Interaction.Behaviors>
                            <h:DataGridAutoColumnBehavior DataGridLength="*" Type="{x:Type h:Student}" />
                            <h:ItemsControlFilterBehavior ItemsSource="{Binding Students}" Order="{Binding ElementName=ob, Path=Order}" />
                        </Behaviors:Interaction.Behaviors>
                    </DataGrid>
                </DockPanel>
            </TabItem>
            <TabItem Header="ZoomViewbox">
                <DockPanel>
                    <!--<h:PropertyGrid Width="300"
                                    h:Cattach.TitleWidth="150"
                                    DockPanel.Dock="Right"
                                    HideInheritedProperties="True"
                                    SelectedObject="{Binding ElementName=zoomview}" />-->
