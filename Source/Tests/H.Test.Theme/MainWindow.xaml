                <FontIconButton Command="{ShowColorThemeViewCommand}"
                                FontFamily="{DynamicResource {x:Static SystemKeys.FontFamilyIcon}}"
                                Style="{DynamicResource {x:Static FontIconButtonKeys.Command}}" />
            </DockPanel>
        </ControlTemplate>
    </h:MainWindow.CaptionTempate>
