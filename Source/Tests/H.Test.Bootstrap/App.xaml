<h:ApplicationBase x:Class="H.Test.Bootstrap.App"
                   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                   xmlns:h="https://github.com/HeBianGu"
                   xmlns:local="clr-namespace:H.Test.Bootstrap">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <FontSizeTheme Type="{x:Static h:FontSizeThemeType.Default}" />
                <LayoutTheme Type="{x:Static h:LayoutThemeType.Default}" />
                <ColorTheme Type="{x:Static h:ColorThemeType.Dark}" />
                <BootstrapStyle />
                <!--<ResourceDictionary Source="pack://application:,,,/H.Styles.Default;component/ConciseControls.xaml" />-->
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</h:ApplicationBase>
