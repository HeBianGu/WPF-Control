<Window x:Class="H.Test.SplashScreen.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:H.Test.SplashScreen"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="MainWindow"
        Width="900"
        Height="550"
        Style="{StaticResource {x:Static WindowKeys.Default}}"
        mc:Ignorable="d">
    <UniformGrid>
        <FontIconButton Command="{ShowSettingCommand}"
                        Style="{DynamicResource {x:Static FontIconButtonKeys.Command}}" />
    </UniformGrid>
</Window>
