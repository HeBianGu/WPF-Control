<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:H.Controls.Form.PropertyItem.EnumerablePropertyItems">
    <DataTemplate DataType="{x:Type local:EnumerablePropertyItem}">
        <ItemsControl Margin="0,0,0,1"
                      ItemsSource="{Binding Value}" />
    </DataTemplate>
</ResourceDictionary>