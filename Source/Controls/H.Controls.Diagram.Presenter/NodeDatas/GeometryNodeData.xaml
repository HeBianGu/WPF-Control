<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:b="http://schemas.microsoft.com/xaml/behaviors"
                    xmlns:h="https://github.com/HeBianGu"
                    xmlns:ln="clr-namespace:H.Controls.Diagram.Presenter.NodeDatas"
                    xmlns:local="clr-namespace:H.Controls.Diagram.Presenter">
    <DataTemplate DataType="{x:Type ln:GeometryNodeDataBase}">
        <Border Background="Transparent"
                BorderBrush="{x:Null}"
                BorderThickness="0"
                Style="{DynamicResource {x:Static local:DiagramKeys.StateBorder}}">
            <Grid>
                <Path Style="{DynamicResource {x:Static local:DiagramKeys.StateNodePath}}" />
                <TextBox Style="{DynamicResource {x:Static local:DiagramKeys.EditTextBox}}" />
            </Grid>
        </Border>
    </DataTemplate>
</ResourceDictionary>