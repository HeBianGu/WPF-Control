<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:h="https://github.com/HeBianGu"
                    xmlns:local="clr-namespace:H.Controls.Form">
    <DataTemplate DataType="{x:Type local:FormPresenter}">
        <local:Form ExceptPropertyNames="{Binding ExceptPropertyNames}"
                    SelectObject="{Binding SelectObject}"
                    TitleWidth="{Binding TitleWidth}"
                    UseArray="{Binding UseArray}"
                    UseAsync="False"
                    UseBoolen="{Binding UseBoolen}"
                    UseClass="{Binding UseClass}"
                    UseCommand="{Binding UseCommand}"
                    UseCommandOnly="{Binding UseCommandOnly}"
                    UseDateTime="{Binding UseDateTime}"
                    UseDeclaredOnly="{Binding UseDeclaredOnly}"
                    UseDisplayOnly="{Binding UseDisplayOnly}"
                    UseEnum="{Binding UseEnum}"
                    UseEnumerator="{Binding UseEnumerator}"
                    UseGroup="{Binding UseGroup}"
                    UseGroupNames="{Binding UseGroupNames}"
                    UseInterface="{Binding UseInterface}"
                    UseNull="{Binding UseNull}"
                    UseOrder="{Binding UseOrder}"
                    UseOrderByName="{Binding UseOrderByName}"
                    UseOrderByType="{Binding UseOrderByType}"
                    UsePresenter="{Binding UsePresenter}"
                    UsePrimitive="{Binding UsePrimitive}"
                    UsePropertyView="{Binding UsePropertyView}"
                    UseString="{Binding UseString}"
                    UseTypeConverter="{Binding UseTypeConverter}"
                    UseTypeConverterOnly="{Binding UseTypeConverterOnly}" />
    </DataTemplate>
</ResourceDictionary>