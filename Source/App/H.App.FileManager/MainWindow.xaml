                                </DrawingImage.Drawing>
                            </DrawingImage>
                        </h:OutlookSection.Image>
                        <h:FavoriteBox SelectedFavoritePath="{Binding Collection.SelectedItem.Model.FavoritePath, Mode=TwoWay}" />
                    </h:OutlookSection>
                </h:OutlookBar.Sections>
            </h:OutlookBar>
            <ContentPresenter Content="{Binding}" />
        </DockPanel>
    </Grid>
</h:MainWindow>
